@model GestionPrj.Models.RScrum

<div class="container">
    <h2>Modifier Reunion Scrum</h2>
    <form asp-action="Update" asp-controller="RScrum" method="post">
        <div class="form-group">
            <label asp-for="Type">Type</label>
            <input type="text" asp-for="Type" class="form-control" id="Description" placeholder="Entrez le Tupe">
            <span asp-validation-for="Type" class="text-danger"></span></>
        </div>
        <div class="form-group">
            <label asp-for="Date">Date</label>
            <input type="Date" asp-for="Date" class="form-control" id="Description" placeholder="Entrez la Date">
            <span asp-validation-for="Date" class="text-danger"></span></>
        </div>
        <div class="form-group">
            <label asp-for="HeureDebut">HeureDebut</label>
            <input type="time" asp-for="HeureDebut" class="form-control" id="Description" placeholder="Entrez la date debut">
            <span asp-validation-for="HeureDebut" class="text-danger"></span></>
        </div>
        <div class="form-group">
            <label asp-for="HeureFin">HeureFin</label>
            <input type="time" asp-for="HeureFin" class="form-control" id="Description" placeholder="Entrez la date fin">
            <span asp-validation-for="HeureFin" class="text-danger"></span></>
        </div>
        <div class="form-group">
            <label asp-for="Description">Description</label>
            <input type="text" asp-for="Description" class="form-control" id="Description" placeholder="Entrez la Description">
            <span asp-validation-for="Description" class="text-danger"></span></>
        </div>

        <button type="submit" class="btn btn-primary">Modifier</button>
    </form>
</div>